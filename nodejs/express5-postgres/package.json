{
  "name": "express5-postgres",
  "version": "1.1.0",
  "description": "Express 5 + PostgreSQL + OpenTelemetry Example",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/jobs/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "node dist/jobs/worker.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:api": "bash scripts/test-api.sh",
    "test:websocket": "node scripts/test-websocket.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "build:lint": "npm run build && npm run lint"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.68.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.210.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.210.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.210.0",
    "@opentelemetry/instrumentation-socket.io": "^0.56.0",
    "@opentelemetry/resources": "^2.4.0",
    "@opentelemetry/sdk-logs": "^0.210.0",
    "@opentelemetry/sdk-node": "^0.210.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.34.8",
    "drizzle-orm": "^0.38.3",
    "express": "^5.2.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.18.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "socket.io": "^4.8.3",
    "zod": "^3.24.1"
  },
  "overrides": {
    "qs": "^6.14.2",
    "esbuild": "^0.27.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.17.0",
    "globals": "^17.0.0",
    "socket.io-client": "^4.8.3",
    "tsx": "^4.21.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vitest": "^4.0.16"
  }
}
